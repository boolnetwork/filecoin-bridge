  chi = i;
  chi2 = znchardecompose(G, chi, 2);
  chi3 = znchardecompose(G, chi, 3);
  print(chi2,", ",chi3);
  if (znconreyexp(G, charmul(G,chi2,chi3)) != i, error(i)))
}

