test(G, chi)=
{
  N = znconreyconductor(G, chi);
  N0 = znconreyconductor(G, chi, &chi0);
  if (N != N0, error(chi));
  print([N0, chi0]);
  G0 = znstar(N0,1); L = znconreylog(G,chi);
  if (zncharinduce(G0, chi0, G.mod) != L, error(chi));
  if (zncharinduce(G0, chi0, G) != L, error(chi));
}
