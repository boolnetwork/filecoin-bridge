T=polcyclo(5,'t); zT = polroots(T)[1];
bestapprnf(sqrt(5), T)
bestapprnf(x*zT+sqrt(5), T, zT)
bestapprnf([exp(2*I*Pi/5)+2], T, zT)
z=sqrt(2);
bestapprnf(z+1, x^2-2)
bestapprnf(z+1, 4*x^2-2)
