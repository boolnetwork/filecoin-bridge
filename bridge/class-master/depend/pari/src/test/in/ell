testzellQp(e, P)=
{ my(a,q,Q);
  a = ellpointtoz(e,P);
  Q = ellztopoint(e,a);
  if (liftall(Q-P),error([e,P]));
  print(a);
  q = e.tate[3];
  a = a^2;
  if (type(a) == "t_PADIC",
    a /= q^(valuation(a,a.p) \ valuation(q,a.p)); /* reduce mod q^Z */
  );
  a - ellpointtoz(e, elladd(e,P,P));
}
