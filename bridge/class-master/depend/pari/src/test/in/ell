twisttest(c4,c6, T)=
{ my(e = ellinit([c4/-48, c6/-864]));
  vector(#T, i,
    my (et = ellinit(elltwist(e, T[i])));
    my (d = ellminimaltwist(et));
    et = ellinit(elltwist(et,d));
    [d, ellminimalmodel(et).disc]);
}
