g(v)=
{
  my(a,[b,c]=v,d=v,e=0,f=I);
  [a,b,c,d,e,f];
}
h(v)=
{
  local(a,[b,c]=v,d=v,e=0,f=I);
  [a,b,c,d,e,f];
}
g([3,4])
h([3,4])

