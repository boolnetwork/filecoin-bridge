galoistest(P,f)=
{
  my(G,F,L);
  G=galoisinit(P);
  F=galoissubfields(G,f);
  for (i=1,#F, my(L=F[i]);
    if (subst(L[1],variable(L[1]),L[2])!=0,
       error("galoissubfields1"));
    if (f==2 && factorback(L[3]*Mod(1,L[1]))!=P,
       error("galoissubfields2")));
}
