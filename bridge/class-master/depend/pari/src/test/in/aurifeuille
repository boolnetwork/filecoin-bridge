alias(factor_Aurifeuille,"_factor_Aurifeuille")
do(d, a) = polcyclo(d, a) / factor_Aurifeuille(a, d);

do(35, -7*3^2)
do(35, 5*3^2*7^2)

do(70, 7*3^2)
do(70, -5*3^2)

do(44, 2*11*9^2)
do(44, 2*11)
do(12,6)
do(4,8)

do(100,2)

alias(factor_Aurifeuille_prime,"_factor_Aurifeuille_prime")
do(d, a) = polcyclo(d, a) / factor_Aurifeuille_prime(a, d);
do(12, 2)
do(15, 5)
do(30, 3)
