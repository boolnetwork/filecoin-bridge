V=vector(3,i,[3/2,3]);
forvec(v=V,print(v))
print()
V=vector(3,i,[3/2,4]);
forvec(v=V,print(v),1)
print()
forvec(v=V,print(v),2)
V=vector(3,i,[1,3]);
forvec(v=V,print(v))
print()
forvec(v=V,print(v),1)
print()
forvec(v=V,print(v),2)
forvec(v=[[1,2],[0,1]],print(v),1)
forvec(v=[[1,2],[0,1]],print(v),2)

forvec(v=vector(3,i,[3/2,7/2]),print(v),1)
print()
forvec(v=[[3/2,3],[2,4],[3/2,5]],print(v),2)

forvec(v=[],print(v))
forvec(v=[1],)
forvec(v=[[1]],)
forvec(v=[[1,I]],)
forvec(v=[[1,1],[1,1]],,3)
