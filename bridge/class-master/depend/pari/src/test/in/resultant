check(P,Q)=
{
  my(d=poldegree(P),e=poldegree(Q));
  if(polresultant(P,Q)*y^(d*e)!=polresultant(subst(P,x,'x/y)*y^d,subst(Q,'x,x/y)*y^e),error(P));
}
