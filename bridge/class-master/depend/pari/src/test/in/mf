\\ Half-integral weight
dim2(N,k,space)=
{
  my(L=mfdim([N,k+1/2,0],space));
  if (!k && space==1,
    for(i=1,#L,print1(L[i][2..3]," ")),
    for(i=1,#L,print1(L[i][3]," "))
  );
}
for(N=1,300,dim2(4*N,0,1));print();
