mtk = [0,0,0,y;0,0,[1,2]~,0;0,1,0,0;1,0,0,0];
mt = [matid(4),mti,mtj,mtk];
al = alginit(nf,mt);
a1 = [1/2,-3,2,4/3,-1,0,1,2/5]~;
a2 = [1+2*y,y/3,5/2,-3*y/7]~*Mod(1,y^2-5);
a3 = algrandom(al,2)/13;
