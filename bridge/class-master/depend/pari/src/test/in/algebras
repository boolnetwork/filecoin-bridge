\\algpoleval type check
setrand(1);
nf = nfinit(y^2-5);
al = alginit(nf,[y,-1]);
a = [1..8]~;
pol = algcharpoly(al,a);
algpoleval(al,pol,a)==0
algpoleval(al,pol,[;])
pol = algcharpoly(al,a,,1);
algpoleval(al,pol,a)==0
ma = algtomatrix(al,a,1);
algpoleval(al,pol,[a,ma])==0

