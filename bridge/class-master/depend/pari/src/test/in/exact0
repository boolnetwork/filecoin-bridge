{
v = [[0,0,0,0], [0,0,0,Mod(0,2)], [1,0,0,Mod(0,2)],
     [1,0,0.,Mod(0,2)], [1,0,Mod(0,2),0.]];
for (i = 1, #v,
  w = v[i];
  print1(Pol(w), " ");
  print1(Polrev(w), " ");
  print1(Ser(w), " ");
  w = vecextract(w, "-1..1");
  print1(Pol(w), " ");
  print1(Polrev(w), " ");
  print1(Ser(w), " "); print();
)
}
